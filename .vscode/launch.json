    "version": "0.2.0",
    "configurations": [
      {
        "type": "lldb",
        "request": "launch",
        "name": "Debug executable 'rust-vector-db-demo'",
        "cargo": {
          "args": [
            "build",
            "--bin=rust-vector-db-demo",
            "--package=rust-vector-db-demo"
          ],
          "filter": {
            "name": "rust-vector-db-demo",
            "kind": "bin"
          }
        },
        "args": [],
        "cwd": "${workspaceFolder}"
      },
      {
        "type": "lldb",
        "request": "launch",
        "name": "Debug unit tests in executable 'rust-vector-db-demo'",
        "cargo": {
          "args": [
            "test",
            "--no-run",
            "--bin=rust-vector-db-demo",
            "--package=rust-vector-db-demo"
          ],
          "filter": {
            "name": "rust-vector-db-demo",
            "kind": "bin"
          }
        },
        "args": [],
        "cwd": "${workspaceFolder}"
      }
    ]
  }